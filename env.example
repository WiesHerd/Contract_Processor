# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_AWS_ACCESS_KEY_ID=your_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_secret_key_here

# S3 Configuration
VITE_S3_BUCKET=your-bucket-name-here

# DynamoDB Table Names (auto-generated by Amplify)
VITE_DYNAMODB_TEMPLATE_TABLE=Template-xxxxxxxxx
VITE_DYNAMODB_PROVIDER_TABLE=Provider-xxxxxxxxx
VITE_DYNAMODB_MAPPING_TABLE=Mapping-xxxxxxxxx
VITE_DYNAMODB_CLAUSE_TABLE=Clause-xxxxxxxxx
VITE_DYNAMODB_AUDIT_TABLE=AuditLog-xxxxxxxxx

# AppSync Configuration (handled by Amplify)
# VITE_APPSYNC_URL=your-appsync-url
# VITE_APPSYNC_API_KEY=your-api-key

# Application Configuration
VITE_APP_NAME=Contract Generator
VITE_APP_VERSION=1.0.0
VITE_MAX_FILE_SIZE=10485760 # 10MB in bytes
VITE_MAX_BULK_UPLOAD=1000 # Maximum providers per bulk upload

# Feature Flags
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_AUDIT_LOGGING=true
VITE_ENABLE_BULK_OPERATIONS=true 